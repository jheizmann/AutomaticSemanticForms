<?xml version="1.0" encoding="UTF-8"?>
<deploydescriptor>
	<global>
		<version>130</version>
		<id>automaticsemanticforms</id>
		<vendor>ontoprise GmbH</vendor>
		<maintainer>ontoprise GmbH</maintainer>
		<instdir>extensions/AutomaticSemanticForms</instdir>
		<description>
			Automatically creates Semantic Forms based on the wiki ontology.
		</description>
		<helpurl>http://smwforum.ontoprise.com/smwforum/index.php/Help:Automatic_Semantic_Forms_Extension</helpurl>
        <license>GPL-v2</license>
		<dependencies>
			<dependency from="160">smwhalo</dependency>
			<dependency from="221">semanticforms</dependency>
			<dependency from="153" to="156" optional="true">smwplus</dependency>
			<dependency from="100" to="100" optional="true">smwplussandbox</dependency>
		</dependencies>
		<patchlevel>0</patchlevel>
		<notice/> 
	</global>
	<codefiles>
		<file loc="deploy.xml" />
	</codefiles>
	<wikidumps>
		<file loc="import/dump.xml"/>
	</wikidumps>
	<resources>
		<!-- empty -->
	</resources>

	<configs>

		<new>
			<require name="asf_init"
				file="extensions/AutomaticSemanticForms/includes/ASF_Initialize.php" />
			<function name="enableAutomaticSemanticForms">
			</function>
		</new>
		<uninstall>

		</uninstall>
	</configs>
</deploydescriptor>

